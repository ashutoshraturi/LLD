CREATE TABLE IF NOT EXISTS groups (entity_id uuid, entity_name text, group_address frozen<address_type>, group_admins frozen<group_admin_type>, group_avatar_url text, group_background_url text, group_location frozen<location_type>, group_members frozen<group_member_type>, group_name text, group_summary text, group_thumbnail_url text, PRIMARY KEY (entity_id, entity_name)) WITH CLUSTERING ORDER BY (entity_name ASC);