CREATE TABLE IF NOT EXISTS pages (entity_id uuid, entity_name text, followers frozen<follower_type>, page_address frozen<address_type>, page_admins frozen<page_admin_type>, page_avatar_url text, page_background_url text, page_location frozen<location_type>, page_name text, page_summary text, page_thumbnail_url text, PRIMARY KEY (entity_id, entity_name)) WITH CLUSTERING ORDER BY (entity_name ASC);