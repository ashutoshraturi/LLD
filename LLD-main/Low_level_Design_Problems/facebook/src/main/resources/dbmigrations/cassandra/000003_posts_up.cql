CREATE TABLE IF NOT EXISTS posts (posted_by_entity_id uuid, post_timestamp date, post_id uuid, posted_on_entity_id uuid, comments frozen<comment_type>, image_url inet, is_live_stream boolean, post_liked_by frozen<post_liked_by_type>, post_shared_by frozen<post_shared_by_type>, post_text text, stream_url inet, video_url inet, PRIMARY KEY (posted_by_entity_id, post_timestamp, post_id, posted_on_entity_id)) WITH CLUSTERING ORDER BY (post_timestamp ASC, post_id ASC, posted_on_entity_id ASC);